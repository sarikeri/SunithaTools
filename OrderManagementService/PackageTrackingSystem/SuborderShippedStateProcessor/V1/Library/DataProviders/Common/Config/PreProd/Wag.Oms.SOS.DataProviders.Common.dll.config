<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>

  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <typeAliases>

      <!--Provider Aliases-->
      <!--=================================================================================-->

      <!--Contracts-->
      <typeAlias alias="ISuborderProvider" type="Wag.Oms.SOS.DataProviders.Contracts.ISuborderProvider, Wag.Oms.SOS.DataProviders.Contracts" />
      <typeAlias alias="IShippedSuborderProvider" type="Wag.Oms.SOS.DataProviders.Contracts.IShippedSuborderProvider, Wag.Oms.SOS.DataProviders.Contracts" />
      <!--Providers-->
      <typeAlias alias="SuborderProvider" type="Wag.Oms.SOS.Providers.SuborderProvider, Wag.Oms.SOS.Providers" />
      <typeAlias alias="ShippedSuborderProvider" type="Wag.Oms.SOS.Providers.ShippedSuborderProvider, Wag.Oms.SOS.Providers" />
      <!--Repository Aliases-->
      <!--=================================================================================-->

      <!--Contracts-->
      <typeAlias alias="ISuborderLoader" type="Wag.Oms.SOS.DAL.Contracts.ISuborderLoader, Wag.Oms.SOS.DAL.Contracts" />
      <typeAlias alias="IShippedSuborderRepository" type="Wag.Oms.SOS.DAL.Contracts.IShippedSuborderRepository, Wag.Oms.SOS.DAL.Contracts" />
      <!--DB Repositories-->
      <typeAlias alias="SuborderLoader" type="Wag.Oms.SOS.DAL.Repositories.SuborderLoader, Wag.Oms.SOS.DAL.Repositories" />
      <typeAlias alias="ShippedSuborderRepository" type="Wag.Oms.SOS.DAL.Repositories.ShippedSuborderRepository, Wag.Oms.SOS.DAL.Repositories" />

    </typeAliases>
    <container>

      <!--Repository Mappings-->
      <!--=================================================================================-->

      <!--DB Repositories-->

      <register type="ISuborderLoader" mapTo="SuborderLoader" name="DB_Rep">

      </register>

      <register type="IShippedSuborderRepository" mapTo="ShippedSuborderRepository" name="DB_Rep">

      </register>

      <!--Provider Mappings-->
      <!--=================================================================================-->

      <register type="ISuborderProvider" mapTo="SuborderProvider">
        <!--Property Injection-->
        <property name="SuborderLoader" dependencyName="DB_Rep" dependencyType="ISuborderLoader" />
      </register>

      <register type="IShippedSuborderProvider" mapTo="ShippedSuborderProvider">
        <!--Property Injection-->
        <property name="ShippedSuborderRepository" dependencyName="DB_Rep" dependencyType="IShippedSuborderRepository" />
      </register>

    </container>
  </unity>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
</configuration>